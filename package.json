{
  "name": "beefy-api",
  "version": "1.0.0",
  "repository": "beefyfinance/beefy-api",
  "license": "MIT",
  "scripts": {
    "install-all": "yarn && cd packages/address-book && yarn",
    "start": "ts-node src/app.ts",
    "dev": "nodemon -e js,ts,json src/app.ts",
    "test": "nyc ava",
    "prettier:check": "prettier --check src/**/*",
    "prettier:fix": "prettier --write src/**/*",
    "prepare": "husky install",
    "bsc:pancake:add": "ts-node scripts/add-farm.ts --network bsc --project pancake",
    "bsc:wault:add": "ts-node scripts/add-farm.ts --network bsc --project wault",
    "bsc:mdex:add": "ts-node scripts/add-farm.ts --network bsc --project mdex",
    "bsc:ape:add": "ts-node scripts/add-farm.ts --network bsc --project ape",
    "bsc:biswap:add": "ts-node scripts/add-farm.ts --network bsc --project biswap",
    "bsc:omni:add": "ts-node scripts/add-omni.ts --network bsc --project omni",
    "fantom:spooky:add": "ts-node scripts/add-farm.ts --network fantom --project spooky",
    "polygon:quick:add": "ts-node scripts/add-quick.ts --network polygon --project quick",
    "polygon:sushi:add": "ts-node scripts/add-sushi.ts --network polygon --project sushiPoly",
    "one:sushi:add": "ts-node scripts/add-sushi.ts --network one --project sushiOne",
    "arb:sushi:add": "ts-node scripts/add-sushi.ts --network arbitrum --project sushiArb",
    "celo:sushi:add": "ts-node scripts/add-sushi.ts --network celo --project sushiCelo",
    "avax:pangolin:add": "ts-node scripts/add-quick.ts --network avax --project pangolin",
    "avax:joe:add": "ts-node scripts/add-farm.ts --network avax --project joe",
    "aurora:trisolaris:add": "ts-node scripts/add-farm.ts --network aurora --project trisolaris",
    "moonriver:solarbeam:add": "ts-node scripts/add-farm.ts --network moonriver --project solarbeam",
    "fuse:fusefi:add": "ts-node scripts/add-quick.ts --network fuse --project fusefi",
    "gen-abi-types": "abi-types-generator ./src/abis/matic/Telxchange/StakingRewards/StakingRewards.json --output='./src/abis/matic/Telxchange/StakingRewards/types'"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^9.4.0",
    "@types/node": "^15.12.2",
    "apollo-link-http": "^1.5.17",
    "apollo-link-timeout": "^4.0.0",
    "axios": "^0.20.0",
    "bignumber.js": "^9.0.1",
    "date-fns": "^2.21.3",
    "eth-multicall": "^1.3.13",
    "ethereum-abi-types-generator": "^1.1.7",
    "ethers": "^5.0.26",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "husky": "^6.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^5.2.0",
    "koa-router": "^9.4.0",
    "node-fetch": "^2.6.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "dotenv": "^8.2.0",
    "lint-staged": "^12.1.4",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "yargs": "^17.0.1"
  },
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.js": "prettier --write",
    "*.json": "prettier --write"
  }
}
