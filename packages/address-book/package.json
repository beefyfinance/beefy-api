{
  "name": "@beefyfinance/blockchain-addressbook",
  "version": "0.54.178",
  "description": "A collection of useful addresses on various chains for defi development",
  "type": "commonjs",
  "types": "./build/address-book/index.d.ts",
  "exports": {
    ".": {
      "import": "./build-esm/address-book/index.js",
      "require": "./build/address-book/index.js"
    },
    "./*": {
      "import": "./build-esm/address-book/*/index.js",
      "require": "./build/address-book/*/index.js"
    },
    "./build/types/token": {
      "import": "./build-esm/types/token.js",
      "require": "./build/types/token.js"
    }
  },
  "main": "./build/address-book/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./build",
    "./build-esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beefyfinance/beefy-api.git"
  },
  "packageManager": "npm@11.6.2",
  "license": "MIT",
  "scripts": {
    "addChain": "bun run ./scripts/addChain.ts",
    "compile": "npm run compile:cjs && npm run compile:esm",
    "compile:cjs": "tsc --project tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > ./build/package.json",
    "compile:esm": "tsc --project tsconfig.esm.json && echo '{\"type\": \"module\"}' > ./build-esm/package.json",
    "checksum": "bun run ./scripts/checksum.ts",
    "checkDecimals": "bun run ./scripts/checkDecimals.ts",
    "checkOracles": "bun run scripts/checkOracles.ts",
    "checkDuplicates": "bun run ./scripts/checkDuplicates.ts",
    "submit": "npm run checksum && npm run compile && npm publish",
    "test": "npm run test:ts && npm run test:lint",
    "test:ts": "npm run test:ts:cjs && npm run test:ts:esm",
    "test:ts:cjs": "tsc --noEmit --project tsconfig.cjs.json",
    "test:ts:esm": "tsc --noEmit --project tsconfig.esm.json",
    "test:lint": "npm run lint",
    "lint": "npm run lint:eslint:check && npm run lint:prettier:check",
    "lint:prettier:check": "prettier --check ./{src,scripts}/**/*.ts",
    "lint:prettier:fix": "prettier --write ./{src,scripts}/**/*.ts",
    "lint:eslint:check": "eslint",
    "lint:eslint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@ethereumjs/util": "^9.0.3",
    "@types/node": "^24.10.3",
    "bun": "^1.3.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.49.0"
  }
}
